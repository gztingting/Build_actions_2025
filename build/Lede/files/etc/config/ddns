
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'Cloudflare'
	option use_ipv6 '1'
	option service_name 'cloudflare.com-v4'
	option enabled '1'
	option lookup_host 'flystation.timesgroup.cc'
	option domain 'flystation@timesgroup.cc'
	option username 'lihaoman74@gmail.com'
	option password 'b2d90a2826ad4b9c7f37f060163879f55e164'
	option use_https '1'
	option cacert 'IGNORE'
	option ip_source 'network'
	option use_syslog '2'
	option ip_network 'wan6'
	option interface 'wan6'

