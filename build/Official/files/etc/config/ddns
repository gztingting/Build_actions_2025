
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'dynv6'
	option use_ipv6 '1'
	option service_name 'dynv6.com'
	option password 'VY2Z8fM8TYzCjLL77XZrb3HU-GPFsm'
	option domain 'xiongduowei.dns.army'
	option enabled '1'
	option lookup_host 'dynv6.com'
	option username 'none'
	option ip_source 'web'
	option use_syslog '2'
	option interface 'wan_6'
	option ip_url 'http://checkipv6.dyndns.com/'

config service 'Cloudflare'
	option use_ipv6 '1'
	option service_name 'cloudflare.com-v4'
	option use_https '1'
	option cacert 'IGNORE'
	option interface 'wan_6'
	option ip_source 'interface'
	option domain 'xdw@smartlocker.cc'
	option lookup_host 'xdw.smartlocker.cc'
	option username 'wonglitermen@gmail.com'
	option password '67cNa0d8nwa9GGbxHuRovi5K2uufgsaENOCc_MMj'
	option enabled '1'
	option use_syslog '2'
	option ip_interface 'pppoe-wan'
	option check_interval '1'
	option check_unit 'hours'
	option force_interval '12'
	option force_unit 'hours'
	option retry_interval '5'
	option retry_unit 'minutes'

config service 'ddynv6'
	option service_name 'dynv6.com'
	option use_ipv6 '1'
	option enabled '1'
	option lookup_host 'ipv6.dynv6.com'
